{
  "name": "node-docker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "MONGO_URL=$MONGO_URL npx migrate-mongo up",
    "migrate-down": "MONGO_URL=$MONGO_URL npx migrate-mongo down",
    "start": "node index.js",
    "dev": "nodemon -L index.js",
    "worker": "nodemon -L utils/worker/index.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:raffle": "jest tests/raffle.test.js",
    "test:vrf": "jest tests/vrf.test.js",
    "test:asset": "jest tests/assetValidation.test.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "validate-config": "node -e \"require('./utils/configValidator').validateConfigurationOrExit()\"",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "docker:dev": "docker-compose -f docker-compose.development.yml up",
    "docker:staging": "docker-compose -f docker-compose.staging.yml up",
    "docker:prod": "docker-compose -f docker-compose.production.yml up",
    "setup:bash": "bash scripts/setup-env.sh",
    "setup:powershell": "powershell -ExecutionPolicy Bypass -File scripts/setup-env.ps1",
    "setup:windows": "npm run setup:powershell",
    "setup:unix": "npm run setup:bash",
    "setup:node": "node setup.js",
    "setup": "node setup.js",
    "make-executable": "echo 'Use: npm run setup (works on all platforms)'",
    "init:points": "node scripts/initializePointsSystem.js",
    "test:points": "jest tests/pointsSystem.test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
